    {
      "parameters": {
        "jsCode": "const results = items[0].json.organic || [];\n\nreturn results.map(profile => {\n  const title = profile.title || \"\";\n  const parts = title.split(\" - \");\n\n  const name = parts[0]?.trim() || null;\n  const position = parts[1]?.trim() || null;\n  const company = parts[2]?.replace(/Founder\\s*/i, \"\").trim() || null;\n\n  return {\n    json: {\n      name,\n      position,\n      company,\n      link: profile.url || null,\n      destination: profile.destination || null,\n      description: profile.description || null\n    }\n  };\n});\n\n"
      },
